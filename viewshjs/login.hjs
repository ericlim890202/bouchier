<!DOCTYPE html>
<html lang="en" ng-app="loginApp">
<head>
    <meta charset="utf-8" />
    <meta name="apple-mobile-web-app-capable" content="yes">
    <title>Peter Bouchier - Butchers of Distinction</title>
    <meta name="description" content="Peter Bouchier iPad App" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, minimal-ui" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- for ios 7 style, multi-resolution icon of 152x152 -->
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-barstyle" content="black-translucent">
    <link rel="apple-touch-icon" href="../assets/images/peter-bouchier-favicon.png">
    <meta name="apple-mobile-web-app-title" content="Flatkit">
    <!-- for Chrome on Android, multi-resolution icon of 196x196 -->
    <meta name="mobile-web-app-capable" content="yes">
    <link rel="shortcut icon" sizes="196x196" href="../assets/images/peter-bouchier-favicon.png">

    <!-- style -->
    <link rel="stylesheet" href="../assets/animate.css/animate.min.css" type="text/css" />
    <link rel="stylesheet" href="../assets/font-awesome/css/font-awesome.min.css" type="text/css" />
    <link rel="stylesheet" href="../assets/material-design-icons/material-design-icons.css" type="text/css" />
    <link rel="stylesheet" href="../assets/angular-motion/dist/angular-motion.css" type="text/css" />
    <!-- Suez One Google Font -->
    <link href="https://fonts.googleapis.com/css?family=Suez+One" rel="stylesheet">

    <!-- stylesheet for angularstrap aside MINIFIED VERSION AT COMPLETION -->
    <link rel="stylesheet" href="../assets/bootstrap-additions/dist/bootstrap-additions.css" type="text/css" />

    <link rel="stylesheet" href="../assets/bootstrap/dist/css/bootstrap.min.css" type="text/css" />
    <!-- build:css ../assets/styles/app.min.css -->
    <link rel="stylesheet" href="../assets/styles/app.css" type="text/css" />
    <!-- endbuild -->
    <link rel="stylesheet" href="../assets/styles/font.css" type="text/css" />

</head>
<body class="{{app.setting.bg}}" ng-class="{'container': app.setting.boxed, 'ie': isIE,'smart': isSmart}">
<div class="app ng-scope" >
    <div class="white bg-auto w-full ng-scope"><!-- uiView:  -->
        <div class="fade-in-right-big smooth pos-rlt ng-scope">
            <div class="text-center ng-scope" style="padding-top: 100px">
                <img src="../assets/images/butcher-logo-small.png">
            </div>
            <div class="center-block w-xxl w-auto-xs ng-scope" ng-controller="loginController">
                <div class="p-a-md box-color r text-color m-a">
                    <form name="form" action="login" method="post" class="ng-pristine ng-valid">
                        <div class="md-form-group float-label">
                            <select class="md-input" name="username" required="">
                                <option ng-repeat="user in users" value="{{user.email}}">{{user.name}} {{user.lastName}}</option>
                            </select>
                        </div>
                        <div class="md-form-group">
                            <input type="password" class="md-input" name="password" placeholder="Enter Password"
                                   required="">
                        </div>
                        <button type="submit" class="btn btn-outline rounded b-primary text-primary btn-block p-x-md">
                            Sign in
                        </button>
                    </form>
                </div>

            </div>
        </div>
    </div>
</div>
<script src="../libs/jquery/jquery/dist/jquery.js"></script>
<!-- Bootstrap -->
<script src="../libs/jquery/tether/dist/js/tether.min.js"></script>
<script src="../libs/jquery/bootstrap/dist/js/bootstrap.js"></script>
<!--Angular -->
<script src="../libs/angular/angular/angular.js"></script>
<script src="../libs/angular/angular-animate/angular-animate.js"></script>
<script src="../libs/angular/angular-resource/angular-resource.js"></script>
<script src="../libs/angular/angular-sanitize/angular-sanitize.js"></script>
<script src="../libs/angular/angular-touch/angular-touch.js"></script>
<script src="//ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-cookies.js"></script>


<!-- router
<script src="../libs/angular/angular-ui-router/release/angular-ui-router.js"></script>
-->
<!-- utils -->
<script src="../libs/angular/angular-ui-utils/ui-utils.js"></script>
<!-- restangular-->
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore-min.js"></script>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/restangular/1.5.2/restangular.min.js"></script>
<!-- lazyload
<script src="../libs/angular/oclazyload/dist/ocLazyLoad.js"></script>
-->
<!-- NG file upload -->
<script src="../libs/angular/ng-fileupload/ng-fileupload.min.js"></script>

<!-- App -->
<script src="scripts/app.js"></script>
<script src="scripts/config.js"></script>
<script src="scripts/config.lazyload.js"></script>
<script src="scripts/config.router.js"></script>
<script src="scripts/app.ctrl.js"></script>
<script src="../libs/angular/angular-strap/dist/angular-strap-2.3.8.min.js"></script>
<script src="../libs/angular/angular-strap/dist/angular-strap-2.3.8-tpl.min.js"></script>
<!-- Angular Pagination Directive -->
<script src="../libs/angular/pagination-directive/pagination-directive.js"></script>

<script src="scripts/directives/ui-jp.js"></script>
<script src="scripts/directives/ui-nav.js"></script>
<script src="scripts/directives/ui-scroll-to.js"></script>
<script src="scripts/directives/ui-toggle-class.js"></script>
<script src="scripts/directives/ui-include.js"></script>

<script src="scripts/filters/fromnow.js"></script>
<script src="scripts/services/ui-load.js"></script>
<script>

    var loginApp = angular.module('loginApp',['restangular'])
            .factory('Users', function (Restangular) {
                return Restangular.service('users');
            });

    loginApp.controller('loginController', ['$scope', 'Users', function($scope, Users) {
        Users.getList().then(function (users) {
            $scope.users = users;
        });
    }]);

</script>
<!-- endbuild -->
</body>
</html>
